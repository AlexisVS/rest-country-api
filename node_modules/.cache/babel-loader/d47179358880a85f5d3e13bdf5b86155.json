n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  data () {\r\n    return {\r\n      land: null,\r\n    }\r\n  },\r\n  computed: {\r\n    fetchUrlLand () {\r\n      return `https://restcountries.com/v3.1/name/${this.$route.params.pays}?fullText=true`\r\n    }\r\n  },\r\n  mounted () {\r\n    axios\r\n      .get(this.fetchUrlLand)\r\n      .then(res => {\r\n        this.land = res.data[0];\r\n      })\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components"}]}